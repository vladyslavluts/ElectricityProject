@model IEnumerable<ElectricityProject.DataLayer.DbLayer.StreetPrefix>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StreetPrefixName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsAfter)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StreetPrefixName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsAfter)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StreetPrefixId }) |
            @Html.ActionLink("Details", "Details", new { id=item.StreetPrefixId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StreetPrefixId })
        </td>
    </tr>
}

</table>
